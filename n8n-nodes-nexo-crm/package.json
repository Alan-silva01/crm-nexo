{
    "name": "n8n-nodes-nexo-crm",
    "version": "1.1.0",
    "description": "n8n Community Nodes para o Nexo CRM - Gerencie leads e colunas do Kanban",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "nexo",
        "crm",
        "leads",
        "kanban"
    ],
    "license": "MIT",
    "homepage": "https://github.com/alan-silva01/n8n-nodes-nexo-crm",
    "author": {
        "name": "Alan Silva",
        "email": "alan@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alan-silva01/n8n-nodes-nexo-crm.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/NexoCrmApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/NexoCrm/NexoCrm.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.56.0",
        "gulp": "^4.0.2",
        "n8n-workflow": "*",
        "prettier": "^3.1.0",
        "typescript": "~5.3.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}